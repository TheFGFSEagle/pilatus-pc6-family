			<aerosurface_scale name="Left Aileron Control">
				<input>fcs/roll-trim-sum</input>
				<range>
					<min>-15</min>
					<max>25</max>
				</range>
				<output>fcs/left-aileron-pos-deg</output>
			</aerosurface_scale>

			<aerosurface_scale>
				<input>-fcs/roll-trim-sum</input>
				<range>
					<min>-15</min>
					<max>25</max>
				</range>
				<output>fcs/right-aileron-pos-deg</output>
			</aerosurface_scale>
			
			<aerosurface_scale>
				<input>fcs/left-aileron-pos-deg</input>
				<domain>
					<min>-15</min>
					<max>25</max>
				</domain>
				<range>
					<min>-1</min>
					<max>1</max>
				</range>
				<output>fcs/left-aileron-pos-norm</output>
			</aerosurface_scale>

			<aerosurface_scale>
				<input>fcs/right-aileron-pos-deg</input>
				<domain>
					<min>-15</min>
					<max>25</max>
				</domain>
				<range>
					<min>-1</min>
					<max>1</max>
				</range>
				<output>fcs/right-aileron-pos-norm</output>
			</aerosurface_scale>
			
			<fcs_function>
				<function>
					<quotient>
						<sum>
							<property>fcs/left-aileron-pos-deg</property>
							<property>-fcs/right-aileron-pos-deg</property>
						</sum>
						<value>2</value>
					</quotient>
				</function>
				<output>fcs/aileron-pos-deg</output>
			</fcs_function>
			
			<fcs_function>
				<function>
					<toradians>
						<property>fcs/aileron-pos-deg</property>
					</toradians>
				</function>
				<output>fcs/aileron-pos-rad</output>
			</fcs_function>
			
			<aerosurface_scale>
				<input>fcs/aileron-pos-deg</input>
				<domain>
					<min>-20</min>
					<max>20</max>
				</domain>
				<range>
					<min>-1</min>
					<max>1</max>
				</range>
				<output>fcs/aileron-pos-norm</output>
			</aerosurface_scale>
		</channel>

