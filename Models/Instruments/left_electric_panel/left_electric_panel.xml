		</axis>
		<action>
			<binding>
				<condition>
					<equals>
						<property>controls/electric/battery-generator-switch-lock</property>
						<value type="bool">false</value>
					</equals>
				</condition>
				<command>property-toggle</command>
				<property>controls/electric/battery-switch</property>
			</binding>
			<binding>
				<condition>
