	<!--battery_generator.lock-->   
    	<animation>
		<type>knob</type>
		<object-name>battery_generator.lock</object-name>
		<property>controls/electric/battery-generator-switch-lock</property>
		<factor>-30</factor>
		<axis>
			<x1-m>0.015</x1-m>
			<y1-m>0.006</y1-m>
			<z1-m>0.066</z1-m>
			<x2-m>0.046</x2-m>
			<y2-m>0.006</y2-m>
			<z2-m>0.084</z2-m>
		</axis>
		<action>
			<binding>
				<command>property-toggle</command>
				<property>controls/electric/battery-generator-switch-lock</property>
			</binding>
		</action>
	</animation>
	
 	<!--battery interrupt-->
	<animation>
		<type>knob</type>
