<?xml version="1.0"?>
<PropertyList>

	<path>prop_percent_rpm.ac</path>
	
	<animation>
		<type>material</type>
		<object-name>pprRPM.face</object-name>
		<object-name>pprRPMneedle</object-name>
		<object-name>pprRpm</object-name>
		<object-name>ppr.ind1</object-name>
		<emission>
			<red>0.28</red>
			<green>0.14</green>
			<blue>0.07</blue>
			<factor-prop>systems/electrical/outputs/instrument-lights-norm</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>pprRPMneedle</object-name>
		<property>instrumentation/rpm-percent-indicator/valeur</property>
		<interpolation>
			<entry><ind>0.0</ind><dep>0.0</dep></entry>
			<entry><ind>110.0</ind><dep>295.0</dep></entry>
		</interpolation>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>pprRpm</object-name>
		<property>instrumentation/rpm-percent-indicator/valeur</property>
		<factor>36</factor>
		<center>
			<x-m>0.0040</x-m>
			<y-m>0.0107</y-m>
			<z-m>-0.0126</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

</PropertyList>