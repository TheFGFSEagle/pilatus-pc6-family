<?xml version="1.0"?>

<PropertyList>

 <path>trim.ac</path>

 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>Knob</object-name>
  <emission>
            <red>0.28</red>
            <green>0.14</green>
            <blue>0.07</blue>
            <factor-prop>systems/electrical/outputs/instrument-lights-norm</factor-prop>
        </emission>
 </animation>

    <animation>
        <type>rotate</type>
        <object-name>Knob</object-name>
        <property>controls/flight/elevator-trim</property>
        <interpolation>
            <entry><ind>0.1</ind><dep>-15.0</dep></entry>
            <entry><ind>0.2</ind><dep>-30.0</dep></entry>
		<entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>-0.1</ind><dep>15.0</dep></entry>
            <entry><ind>-0.3</ind><dep>45.0</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
    </animation>

</PropertyList>


