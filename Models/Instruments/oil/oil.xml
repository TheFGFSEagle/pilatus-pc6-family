<?xml version="1.0"?>
<!-- fuel flow 
Syd Adams
-->
<PropertyList>

    <path>oil.ac</path>
    
    <animation>
        <type>material</type>
        <object-name>OILface</object-name>
        <object-name>OILneedleC</object-name>
        <object-name>OILneedlePSI</object-name>
        <emission>
            <red>0.28</red>
            <green>0.14</green>
            <blue>0.07</blue>
            <factor-prop>systems/electrical/outputs/instrument-lights-norm</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>OILneedleC</object-name>
        <property>instrumentation/oil-indicator/temp_valeur</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>120</ind><dep>135.0</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>OILneedlePSI</object-name>
        <property>instrumentation/oil-indicator/psi_valeur</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>150</ind><dep>135.0</dep></entry>
        </interpolation>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

</PropertyList>