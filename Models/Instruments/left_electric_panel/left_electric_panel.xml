	<!--landing light right position-->
	<animation>
		<type>knob</type>
		<object-name>pos_landing_R.lever</object-name>
		<property>controls/lighting/landing-light-pos[1]</property>
		<factor>-30</factor>
		<axis>
			<x1-m>0.068</x1-m>
			<y1-m>0.006</y1-m>
			<z1-m>0.032</z1-m>
			<x2-m>-0.080</x2-m>
			<y2-m>0.006</y2-m>
			<z2-m>-0.053</z2-m>
		</axis>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>sim/model/lights/landing-light[1]/pos-norm</property>
				<factor>0.02</factor>
				<min>0</min>
				<max>1</max>
			</binding>
			<binding>
				<command>property-adjust</command>
				<property>controls/lighting/landing-light-pos[1]</property>
				<factor>1</factor>
				<min>-1</min>
				<max>1</max>
			</binding>
			<binding>
				<condition>
					<not><property>sim/sound/switch</property></not>
				</condition>
				<command>property-assign</command>
				<property>sim/sound/switch</property>
				<value type="bool">true</value>
			</binding>
			<binding>
				<condition>
					<property>sim/sound/switch</property>
				</condition>
				<command>property-assign</command>
				<property>sim/sound/switch</property>
				<value type="bool">false</value>
			</binding>
		</action>
		<release>
			<binding>
				<command>property-assign</command>
				<property>controls/lighting/landing-light-pos[1]</property>
				<value>0</value>
			</binding>
			<binding>
				<command>property-toggle</command>
				<property>sim/sound/switch</property>
			</binding>
		</release>
	</animation>
	
